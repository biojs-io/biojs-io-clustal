  },
  "devDependencies": {
    "browserify": "~4.2.0",
    "coffee-script": "^1.7.1",
    "coffeeify": "^0.7.0",
    "gulp": "^3.8.6",
    "gulp-util": "^3.0.0",
    "mocha": "~1.20.1",
    "nock": "^0.43.0",
